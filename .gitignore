# Ignore directories generated by Composer
#/drush/contrib/
#/vendor/
#/web/core/
#/web/modules/contrib/
#/web/themes/contrib/
#/web/profiles/contrib/
#/web/libraries/
/console/
/drupal-mount/

# Ignore sensitive information
/web/sites/*/services.yml
/web/sites/*/settings.local.php
#default.settings.php
settings.local.php
services.yml
web/sites/default/settings.php
# Ignore Drupal's file directory
/web/sites/*/files/

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Ignore files generated by PhpStorm
/.idea/

# Ignore .env files as they are personal
/.env
web/themes/.DS_Store
web/modules/custom/.DS_Store
web/themes/*/node_modules/
web/themes/*/dist/*.css.map
web/themes/*/dist/*.js.map
*.vscode
.vscode

# Explicitly track compiled assets.
!/web/themes/coe_onecity/dist/main.js
!/web/themes/coe_onecity/dist/main.css
!/web/themes/coe_onecity/dist/social.js
!/web/themes/coe_onecity/dist/social.css

# Ignore docker-related data.
/docker/local-dev/mariadb/data/*
/docker/local-dev/drupal/data/*
!/docker/local-dev/mariadb/data/.gitkeep
/docker/local-dev/drupal/data/*
!/docker/local-dev/drupal/data/.gitkeep
/supervisord.*

# Ignore SQL files
*.sql
*.sql.gz
